-- Script Clase --

--Subconsultas--
--Consulta principal 1--
SELECT PNOMBRE,SUELDO_BASE FROM MEDICO
WHERE SUELDO_BASE>(SELECT SUELDO_BASE FROM MEDICO
WHERE MED_RUN = 3126425)
ORDER BY SUELDO_BASE;

--Subconsulta 1--
SELECT SUELDO_BASE FROM MEDICO
WHERE MED_RUN = 3126425;

--Subconsulta 2--
SELECT AVG(SUELDO_BASE) FROM MEDICO
;
--Consulta principal 2--
SELECT PNOMBRE,SUELDO_BASE FROM MEDICO
WHERE SUELDO_BASE>(SELECT AVG(SUELDO_BASE) FROM MEDICO WHERE UNI_ID=100)
ORDER BY SUELDO_BASE
;

--Consulta principal 3--
SELECT PNOMBRE FROM MEDICO
WHERE PNOMBRE= (SELECT PNOMBRE, (SUELDO_BASE) FROM MEDICO
ORDER BY SUELDO_BASE DESC)
FALTA TERMINAR
;
--Subconsulta 3--
SELECT PNOMBRE, (SUELDO_BASE) FROM MEDICO
ORDER BY SUELDO_BASE DESC;
FALTA TERMINAR












